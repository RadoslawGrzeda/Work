with art as (
     SELECT 
        an.ART_NUMER,
        meti.EAN_CD as ean,
        an.ART_NAZWA
      FROM 
        `pol-it-analytics-2019120200.POL_BD_MARZA.v_dic_artykul_nomenklatura` an
      JOIN 
        `pol-it-wh-prod-2019120200.POL_WH_METI_CENTRAL.raw_mgean` meti
      ON 
        an.ART_NUMER=meti.EAN_NOART
      WHERE
      --   EAN_MAIT='O'
      -- AND  
        ART_NUMER=183986
)
,agr2024 as (
  SELECT 
      th.TrnId as id,
      th.TrnLoyaltyCustomerId as nrKarty,
      TrnLineEAN  as ean,
      TrnLineQuantity as ilosc,
      TrnLineValue as obrot,
      TrnLineValue/2 as cashBack
  FROM 
      `pol-it-cloudtrn-2021042100.TrnCloud.TrnHeader` th
  JOIN
    `pol-it-cloudtrn-2021042100.TrnCloud.TrnLine` tl
  ON 
      th.TrnId=tl.TrnId
  JOIN
      art m 
  ON 
    cast(m.ean as string)=tl.TrnLineEAN
  WHERE 
    th.TrnDate between "2024-03-22" and '2024-03-23'
  AND 
    tl.TrnDate between "2024-03-22" and '2024-03-23'
)

,agr2025 as (
  SELECT 
      th.TrnId as id,
      th.TrnLoyaltyCustomerId as nrKarty,
      TrnLineEAN  as ean,
      TrnLineQuantity as ilosc,
      TrnLineValue as obrot,
      TrnLineValue/2 as cashBack
  FROM 
      `pol-it-cloudtrn-2021042100.TrnCloud.TrnHeader` th
  JOIN
    `pol-it-cloudtrn-2021042100.TrnCloud.TrnLine` tl
  ON 
      th.TrnId=tl.TrnId
  JOIN
      art m 
  ON 
    cast(m.ean as string)=tl.TrnLineEAN
  WHERE 
    th.TrnDate between "2025-03-28" and '2025-03-29'
  AND 
    tl.TrnDate between "2025-03-28" and '2025-03-29'
)

,dane as (

select 
    '2025' as rok,
    'Loy' as typ,
    count(distinct id )as iloscTrans,
    count(distinct nrKarty) as liczbaUnikalnychKlientow,
    sum(ilosc)  as liczbaSprzedanychArt,
    sum(obrot) as obrot   ,
    sum(cashback) as cashback
from 
    agr2025
where
    nrKarty is not null

UNION ALL 

select 
    '2025' as rok,
    'NotLoy' as typ,
    count(distinct id )as iloscTrans,
    count(distinct nrKarty) as liczbaUnikalnychKlientow,
    sum(ilosc)  as liczbaSprzedanychArt,
    sum(obrot) as obrot   ,
    sum(cashback) as cashback
from 
    agr2025  
where 
    nrKarty is null

UNION ALL    

select 
    '2024' as rok,
    'Loy' as typ,
    count(distinct id )as iloscTrans,
    count(distinct nrKarty) as liczbaUnikalnychKlientow,
    sum(ilosc)  as liczbaSprzedanychArt,
    sum(obrot) as obrot   ,
    sum(cashback) as cashback
from 
    agr2024
where
    nrKarty is not null

UNION ALL 

select 
    '2024' as rok,
    'NotLoy' as typ,
    count(distinct id )as iloscTrans,
    count(distinct nrKarty) as liczbaUnikalnychKlientow,
    sum(ilosc)  as liczbaSprzedanychArt,
    sum(obrot) as obrot   ,
    sum(cashback) as cashback
from 
    agr2024  
where 
    nrKarty is null
)

-- Select * from dane 
Select 
a.typ,
a.liczbaUnikalnychKlientow as liczbaUnikalnychKlientow25,
b.liczbaUnikalnychKlientow  as liczbaUnikalnychKlientow24,
SAFE_DIVIDE(a.liczbaUnikalnychKlientow-b.liczbaUnikalnychKlientow,b.liczbaUnikalnychKlientow)*100 as ProcUnikalnychKlientow,
a.iloscTrans as trans25,
b.iloscTrans as trans24,
a.iloscTrans-b.iloscTrans as roznicaTrans,
SAFE_DIVIDE(a.iloscTrans-b.iloscTrans,b.iloscTrans)*100 as ProcTrans,
a.obrot as obrot25,
b.obrot as obrot24,
a.obrot-b.obrot as roznicaObrotu,
SAFE_DIVIDE(a.obrot-b.obrot,b.obrot)*100 as ProcObrot
from 
  dane a 
join
  dane b 
on 
  a.typ=b.typ
where 
  a.rok='2025' 
and 
  b.rok='2024'