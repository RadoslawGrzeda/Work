-- SELECT * FROM `auchan-pol-prod.raw_salesforce_customerbase_sec.community_member` where date_of_unsubscription is not null and community_code='COM_06' LIMIT 1000
DECLARE v_curr date;
set v_curr=current_date();
INSERT INTO `pol-it-digiteam-2022062900.RADEK.SeniorKdrPracownik` 



with AktualnaKarta as
 (SELECT DISTINCT
    ac.person_id
    , ac.person_external_loyalty
    , ac.account_loyalty
    , l.identifier_id AS nr_karty
    , count(*) OVER (partition by l.person_id) AS liczba_kart_sf
    , ai.accountId
    , ai.customerId
  FROM
    `auchan-pol-prod.raw_salesforce_customerbase_sec.account_contact` ac
  JOIN
    `auchan-pol-prod.raw_salesforce_customerbase_sec.loyalty` l
    ON ac.person_id = l.person_id
  JOIN
    `auchan-pol-prod.raw_comarch.account_identifier` ai
    ON ai.no = l.identifier_id
    AND CAST(ai.accountId AS string) = ac.account_loyalty
    AND CAST(ai.customerId AS string) = ac.person_external_loyalty  
    AND ai.status = 'A'
  WHERE
    l.is_deleted = false
    AND l.loyalty_status = 'LOS_01'
 )
 ,Senior as (
  SELECT 
    person_id,
    community_code,
    date_of_subscription,
    flag_actif,
    date_of_unsubscription,
  FROM `auchan-pol-prod.raw_salesforce_customerbase_sec.community_member` 
  where community_code='COM_06'
 )
   ,KDR as (
  SELECT 
    person_id,
    community_code,
    date_of_subscription,
    flag_actif,
    date_of_unsubscription,
  FROM `auchan-pol-prod.raw_salesforce_customerbase_sec.community_member` 
  where community_code='COM_03'
 )
 ,Pracownik as (
  SELECT  
   distinct 
    a.person_id,
    a.type_account_indicator as status,
    value_account_indicator as flaga,
     last_modification_date as date_of_subscritpion
  FROM 
    `auchan-pol-prod.raw_salesforce_customerbase_sec.account_indicator` a
  JOIN 
    `auchan-pol-prod.raw_salesforce_customerbase_sec.loyalty` b 
    ON a.person_id = b.person_id
  LEFT JOIN `auchan-pol-prod.raw_comarch.account_identifier` af 
    ON af.no = b.identifier_ID
  WHERE 
    a.type_account_indicator = 'AIT_04' 
     and a.is_deleted=False 
    AND b.is_deleted=False 
    -- AND a.value_account_indicator in (')
    AND b.loyalty_status='LOS_01' 
    AND af.status='A'
    -- and last_modification_source like '%HR%'
    --  and a.person_id='8850096947204'
    --  and last_modification_date is not null 
 )

,wszystko as (
 SELECT 
  s.person_id,
  'SENIOR' as TypKlienta,
  cast(date_of_subscription as date) as DataSubskrypcji ,
  case when flag_actif=true then 'AKTYWNY' else 'NIEAKTYWNY' end as Status,
  case when flag_actif=false then cast(date_of_unsubscription as date) end as DataUnSubskrypcji,
  v_curr as DataGenerowania
 FROM
  Senior s
 

  UNION ALL

 SELECT 
    kdr.person_id,
    'KDR' as TypKlienta,
    cast(date_of_subscription as date) as DataSubskrypcji ,
    case when flag_actif=true then 'AKTYWNY' else 'NIEAKTYWNY' end as Status,
    case when flag_actif=false then cast (date_of_unsubscription as date) end as DataUnSubskrypcji,
    v_curr as DataGenerowania
 FROM
  KDR kdr

UNION ALL

   SELECT 
    pr.person_id,
    'PRACOWNIK' as TypKlienta,
    cast(date_of_subscritpion as date) as DataSubskrypcji ,
    case when flaga='AIV_01' then 'AKTYWNY' when flaga='AIV_02' then 'NIEAKTYWNY' end as Status,
    cast (date_of_subscritpion as date) as DataUnSubskrypcji,
    v_curr as DataGenerowania
 FROM
  Pracownik pr
  where flaga='AIV_01'
  )

  select * 
  from 
  wszystko w
   where not exists
  (select
   person_id,
   TypKlienta,
   Status 
  from 
  `pol-it-digiteam-2022062900.RADEK.SeniorKdrPracownik` s2 
  where
   s2.person_id=w.person_id and s2.status=w.Status and 
  s2.TypKlienta=w.TypKlienta)

  union all 

  SELECT 
  pr2.person_id,
  'PRACOWNIK' as TypKlienta,
  cast(date_of_subscritpion as date) as DataSubskrypcji ,
  case when flaga='AIV_01' then 'AKTYWNY' when flaga='AIV_02' then 'NIEAKTYWNY' end as Status,
  cast (date_of_subscritpion as date) as DataUnSubskrypcji,
  v_curr as DataGenerowania
 FROM
  Pracownik pr2 
  where pr2.person_id in (select person_id from `pol-it-digiteam-2022062900.RADEK.SeniorKdrPracownik` where Status='PRACOWNIK')
  and flaga='AIV_02'
 
 




    -- AND ac.person_id = '5410840'